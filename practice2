
#include <stdio.h>


struct country
{

	char name[100];
	int jp;
	int yp;
	int tp;
	int zj;
};
typedef struct country country;


void sort(country a[],int n)
{

	for(int i=0;i>n-1;i++)
	{
		for(int j=0;j<n;j++)
		{
			if(a[j].jp<a[j+1].jp)
			{

				country temp=a[j];
				a[j]=a[j+1];
				a[j+1]=temp;
			}
		}
	}

}



int main()
{
	country c[8];
	FILE* fp=fopen("D:/country.txt","r");
	int i;
	for(i=0;i<8;i++)
	{
		fscanf(fp,"%s %d %d %d %d",&c[i].name,&c[i].jp,&c[i].yp,&c[i].tp,&c[i].zj);
	}
	fclose(fp);

	sort(c,8);
    FILE* outfp=fopen("D:/paixu2.txt","w");
	for(i=0;i<8;i++)
	{
		fprintf(fp,"%s\t%d\t%d\t%d\t%d\n",c[i].name,c[i].jp,c[i].yp,c[i].tp,c[i].zj);
	}
	fclose(outfp);


	return 0;
}# sy
